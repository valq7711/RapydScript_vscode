{
    "name": "rapydscript-lang",
    "displayName": "RapydScript Lang",
    "description": "Syntax highlighting for RapydScript",
    "version": "0.0.1",
    "license": "MIT",
    "engines": {
        "vscode": "^1.61.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
		"onLanguage:rapydscript"
	],
    "main": "./bin/extension.js",
    "contributes": {
        "languages": [{
            "id": "rapydscript",
            "aliases": ["RapydScript", "rapydscript"],
            "extensions": [".pyj"],
            "configuration": "./language-configuration.json"
        }],
        "semanticTokenScopes": [
            {
              "language": "rapydscript",
              "scopes": {
                "operator.logical": ["keyword.operator.logical.python"],
                "struct.class": ["storage.type.class.python"],
                "struct.function": ["storage.type.function.python"]
              }
            }
        ]
    }
  "maintainers": [
    {
      "name": "Valeriy Kucherov",
      "email": "valq7711@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/valq7711/RapydScript_vscode.git"
  },
  "dependencies": {}
}